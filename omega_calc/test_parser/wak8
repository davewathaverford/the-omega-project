 T1 := {[i1,i2,i3] -> [o1,i2,o3] : 2 <= i2 <= 511 && o1 <= 256 
&& o3 <= 511 && 2 <= i3 && i1+o3 <= i3+o1 
	&& 255i3+o1 <= i1+255o3 && 1 <= i1};

T1;

 T2 := {[i1,i2,i3] -> [o1,i2,o3] : Exists ( x: 2 <= i2 <= 511 
	&& 2 <= o3 <= 509 && o1 <= 256 
	&& 3+i1+o3 <= i3+o1 && 2o1+o3 <= 511+2x 
	&& 2 <= i3 && i1+2o1+o3 <= i3+3x 
	&& 255i3+511x <= i1+510o1+255o3 && 1 <= i1)};

T2;
 
 T1 compose T2;
