# R+;

{[i,j] -> [Out_1,j'] : Out_1+2j' = i+2j && j' < j} union
 {[i,j] -> [Out_1,j'] : Exists ( alpha : i+j' = j+Out_1+3alpha && 3+i+2j <= Out_1+2j' && i+j' <= j+Out_1)}

